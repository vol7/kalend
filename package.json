{
  "name": "kalend",
  "version": "0.14.18",
  "author": "nibdo",
  "license": "MIT",
  "private": false,
  "source": "src/index.tsx",
  "targets": {
    "default": {
      "distDir": "./dist"
    },
    "styles": {
      "source": "src/index.scss",
      "optimize": false
    }
  },
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "style": "dist/styles/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/nibdo/kalend"
  },
  "dependencies": {
    "@swc/helpers": "0.2.14",
    "color": "4.2.3",
    "kalend-layout": "0.0.16",
    "luxon": "2.4.0",
    "regenerator-runtime": "0.13.9"
  },
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@parcel/packager-ts": "2.0.1",
    "@parcel/transformer-sass": "2.0.1",
    "@parcel/transformer-typescript-types": "2.0.1",
    "@storybook/addon-actions": "6.4.8",
    "@storybook/addon-console": "1.2.3",
    "@storybook/addon-essentials": "6.4.8",
    "@storybook/addon-links": "6.4.8",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.4.8",
    "@types/color": "3.0.3",
    "@types/faker": "5.5.9",
    "@types/jest": "27.0.3",
    "@types/luxon": "2.0.7",
    "@types/mocha": "9.0.0",
    "@types/node": "14.17.34",
    "@types/react": "17.0.35",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "5.4.0",
    "babel-loader": "8.2.3",
    "chai": "4.3.4",
    "copyfiles": "2.4.1",
    "css-loader": "5.2.7",
    "eslint-config-prettier": "5.1.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "faker": "5.5.3",
    "husky": "7.0.4",
    "mocha": "9.1.3",
    "parcel": "2.0.1",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "sass-loader": "10.0.0",
    "style-loader": "2.0.0",
    "ts-mocha": "8.0.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.2"
  },
  "peerDependencies": {
    "react": "17.0.2 - 18",
    "react-dom": "17.0.2 - 18"
  },
  "scripts": {
    "prepare": "npx husky install && npx husky add .husky/pre-commit \"npm run lint\"",
    "tsc-build": "tsc --noEmit",
    "build": "npm run tsc-build && parcel build --detailed-report 5",
    "package": "npm run test && npm run build && npm pack",
    "start": "BROWSER=none PORT=4103 react-scripts start",
    "test": "ts-mocha --config ./mocharc.js",
    "storybook": "start-storybook -p 8080",
    "lint": "eslint . --ext ts --ext tsx"
  },
  "keywords": [
    "javascript"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
